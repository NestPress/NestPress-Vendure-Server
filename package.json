{
  "name": "my-v",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "run:server": "ts-node ./src/index.ts",
    "run:worker": "ts-node ./src/index-worker.ts",
    "dev:server": "ts-node-dev --respawn ./src/index.ts",
    "dev:worker": "ts-node-dev ./src/index-worker.ts",
    "start": "concurrently yarn:run:*",
    "build": "tsc",
    "dev": "concurrently yarn:dev:*",
    "migration:generate": "ts-node migration generate",
    "migration:run": "ts-node migration run",
    "migration:revert": "ts-node migration revert",
    "schema:generate": "ts-node generate-schema"
  },
  "dependencies": {
    "@nestjs/common": "^8.1.2",
    "@nestjs/core": "^8.1.2",
    "@nestjs/graphql": "^9.1.1",
    "@vendure/admin-ui-plugin": "1.3.1",
    "@vendure/asset-server-plugin": "1.3.1",
    "@vendure/core": "1.3.1",
    "@vendure/email-plugin": "1.3.1",
    "@vendure/ui-devkit": "^1.3.2",
    "pg": "8.7.1",
    "typeorm": "^0.2.38",
    "typescript": "4.1.5"
  },
  "devDependencies": {
    "concurrently": "6.3.0",
    "ts-node": "10.3.0",
    "ts-node-dev": "^1.1.6"
  }
}
